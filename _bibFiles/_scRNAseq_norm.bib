	doi = {10.1038/s41467-024-50194-3},
	id = {Zeng2024},
	isbn = {2041-1723},
	journal = {Nature Communications},
	number = {1},
	pages = {5983},
	title = {OmicVerse: a framework for bridging and deepening insights across bulk and single-cell sequencing},
	url = {https://doi.org/10.1038/s41467-024-50194-3},
	volume = {15},
