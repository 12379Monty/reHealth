@article{Ahlmann-Eltze:2023aa,
	abstract = {The count table, a numeric matrix of genes ×cells, is the basic input data structure in the analysis of single-cell RNA-sequencing data. A common preprocessing step is to adjust the counts for variable sampling efficiency and to transform them so that the variance is similar across the dynamic range. These steps are intended to make subsequent application of generic statistical methods more palatable. Here, we describe four transformation approaches based on the delta method, model residuals, inferred latent expression state and factor analysis. We compare their strengths and weaknesses and find that the latter three have appealing theoretical properties; however, in benchmarks using simulated and real-world data, it turns out that a rather simple approach, namely, the logarithm with a pseudo-count followed by principal-component analysis, performs as well or better than the more sophisticated alternatives. This result highlights limitations of current theoretical analysis as assessed by bottom-line performance benchmarks.},
	author = {Ahlmann-Eltze, Constantin and Huber, Wolfgang},
	date = {2023/05/01},
	date-added = {2025-05-19 02:05:20 -0700},
	date-modified = {2025-05-19 02:05:20 -0700},
	doi = {10.1038/s41592-023-01814-1},
	id = {Ahlmann-Eltze2023},
	isbn = {1548-7105},
	journal = {Nature Methods},
	number = {5},
	pages = {665--672},
	title = {Comparison of transformations for single-cell RNA-seq data},
	url = {https://doi.org/10.1038/s41592-023-01814-1},
	volume = {20},
	year = {2023},
	bdsk-url-1 = {https://doi.org/10.1038/s41592-023-01814-1}}

@article{Melms:2021aa,
	abstract = {Respiratory failure is the leading cause of death in patients with severe SARS-CoV-2 infection1,2, but the host response at the lung tissue level is poorly understood. Here we performed single-nucleus RNA sequencing of about 116,000 nuclei from the lungs of nineteen individuals who died of COVID-19 and underwent rapid autopsy and seven control individuals. Integrated analyses identified substantial alterations in cellular composition, transcriptional cell states, and cell-to-cell interactions, thereby providing insight into the biology of lethal COVID-19. The lungs from individuals with COVID-19 were highly inflamed, with dense infiltration of aberrantly activated monocyte-derived macrophages and alveolar macrophages, but had impaired T cell responses. Monocyte/macrophage-derived interleukin-1βand epithelial cell-derived interleukin-6 were unique features of SARS-CoV-2 infection compared to other viral and bacterial causes of pneumonia. Alveolar type 2 cells adopted an inflammation-associated transient progenitor cell state and failed to undergo full transition into alveolar type 1 cells, resulting in impaired lung regeneration. Furthermore, we identified expansion of recently described CTHRC1+ pathological fibroblasts3 contributing to rapidly ensuing pulmonary fibrosis in COVID-19. Inference of protein activity and ligand--receptor interactions identified putative drug targets to disrupt deleterious circuits. This atlas enables the dissection of lethal COVID-19, may inform our understanding of long-term complications of COVID-19 survivors, and provides an important resource for therapeutic development.},
	author = {Melms, Johannes C. and Biermann, Jana and Huang, Huachao and Wang, Yiping and Nair, Ajay and Tagore, Somnath and Katsyv, Igor and Rendeiro, Andr{\'e}F. and Amin, Amit Dipak and Schapiro, Denis and Frangieh, Chris J. and Luoma, Adrienne M. and Filliol, Aveline and Fang, Yinshan and Ravichandran, Hiranmayi and Clausi, Mariano G. and Alba, George A. and Rogava, Meri and Chen, Sean W. and Ho, Patricia and Montoro, Daniel T. and Kornberg, Adam E. and Han, Arnold S. and Bakhoum, Mathieu F. and Anandasabapathy, Niroshana and Su{\'a}rez-Fari{\~n}as, Mayte and Bakhoum, Samuel F. and Bram, Yaron and Borczuk, Alain and Guo, Xinzheng V. and Lefkowitch, Jay H. and Marboe, Charles and Lagana, Stephen M. and Del Portillo, Armando and Tsai, Emily J. and Zorn, Emmanuel and Markowitz, Glen S. and Schwabe, Robert F. and Schwartz, Robert E. and Elemento, Olivier and Saqi, Anjali and Hibshoosh, Hanina and Que, Jianwen and Izar, Benjamin},
	date = {2021/07/01},
	date-added = {2025-05-19 02:05:20 -0700},
	date-modified = {2025-05-19 02:05:20 -0700},
	doi = {10.1038/s41586-021-03569-1},
	id = {Melms2021},
	isbn = {1476-4687},
	journal = {Nature},
	number = {7865},
	pages = {114--119},
	title = {A molecular single-cell lung atlas of lethal COVID-19},
	url = {https://doi.org/10.1038/s41586-021-03569-1},
